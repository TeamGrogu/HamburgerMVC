@model List<Category>


<table class="table table-striped table-bordered table-hover table-responsive text-center table-sm">
    <thead>
        <tr>
            <th>ID</th>
            <th>CategoryName</th>
            <th>Update</th>
            <th>Delete</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Category category in Model)
        {
            <tr>
                <td>@category.ID</td>
                <td>@category.CategoryName</td>
                <td><i class="fas fa-pencil text-warning"></i></td>
                <td><i class="fas fa-trash text-danger"></i></td>
            </tr>
        }
    </tbody>
</table>
